(set-option :produce-proofs true)
(set-logic QF_LIA)
(declare-fun K () Int)
(declare-fun P () Int)
(declare-fun R () Int)
(declare-fun X () Int)
(declare-fun Z () Int)
(assert (and (or (not (or (<= 1 P) (<= 0 K) (<= 1 R))) (and (distinct 0 Z) (= 0 Z)) (= 0 X)) (or (<= 1 P) (<= 0 K) (<= 1 R)) (or (= 1 Z) (< 0 X)) (or (< 0 X) (= 0 Z))))
(check-sat)
(get-proof)